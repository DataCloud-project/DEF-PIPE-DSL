<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="wfDsl" nsURI="http://www.WfDsl.workflow" nsPrefix="wfDsl">
  <eClassifiers xsi:type="ecore:EClass" name="Pipeline">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="abstractstep" lowerBound="1"
        upperBound="-1" eType="#//AbstractStep"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="communicationMedium" eType="#//CommunicationMedium"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractStep" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="triggers" upperBound="-1"
        eType="#//Trigger/Trigger" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="requirements" upperBound="-1"
        eType="#//Environment%20Parameters/ExecutionRequirement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="previous" unique="false"
        upperBound="-1" eType="#//AbstractStep"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controlParemeter" lowerBound="1"
        eType="#//Control%20Parameters/ControlParemeter"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="deploymentParams" eType="#//Deployment%20Parameters%20(Requirements)/DeploymentParameter"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CommunicationMedium">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//CommunicationMediumTypes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StepImplementation"/>
  <eClassifiers xsi:type="ecore:EClass" name="DockerImplementation" eSuperTypes="#//StepImplementation">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="startTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="imageName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Parameter">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parametergroup" eType="#//ParameterGroup"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CommunicationMediumTypes">
    <eLiterals name="MESSAGE_QUEUE" literal="MESSAGE_QUEUE"/>
    <eLiterals name="DISTRIBUTED_FILE_SYSTEM" value="1" literal="DISTRIBUTED_FILE_SYSTEM"/>
    <eLiterals name="WEB_SERVICE" value="2" literal="WEB_SERVICE"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TimeUnits">
    <eLiterals name="SECOND" literal="SECOND"/>
    <eLiterals name="MINUTE" value="1" literal="MINUTE"/>
    <eLiterals name="HOUR" value="2" literal="HOUR"/>
    <eLiterals name="DAY" value="3" literal="DAY"/>
    <eLiterals name="WEEK" value="4" literal="WEEK"/>
    <eLiterals name="MONTH" value="5" literal="MONTH"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="DeploymentLayers">
    <eLiterals name="EDGE" value="1"/>
    <eLiterals name="FOG" value="1"/>
    <eLiterals name="CLOUD" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompositeStep" eSuperTypes="#//AbstractStep">
    <eStructuralFeatures xsi:type="ecore:EReference" name="steps" lowerBound="1" upperBound="-1"
        eType="#//AbstractStep" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StepTemplate" eSuperTypes="#//AbstractStep">
    <eStructuralFeatures xsi:type="ecore:EReference" name="implementation" eType="#//StepImplementation"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="params" upperBound="-1"
        eType="#//Input%20Parameters/DataSource" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ParameterGroup">
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" upperBound="-1"
        eType="#//Parameter" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="parameterOperator" eType="#//LogicalOperatorType"
        defaultValueLiteral="AND"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="LogicalOperatorType">
    <eLiterals name="AND"/>
    <eLiterals name="XOR" value="1"/>
    <eLiterals name="OR" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ComparisonOperatorType">
    <eLiterals name="EQUAL"/>
    <eLiterals name="LESS_THAN" value="1"/>
    <eLiterals name="GREATER_THAN" value="2"/>
    <eLiterals name="NOT_EQAL" value="3"/>
    <eLiterals name="EQUAL_LESS_THAN" value="4"/>
    <eLiterals name="EQUAL_GREATER_THAN" value="5"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="AbstractCondition" instanceClassName="AbstractContition"/>
  <eClassifiers xsi:type="ecore:EDataType" name="Step" instanceTypeName="Step"/>
  <eClassifiers xsi:type="ecore:EClass" name="Step" eSuperTypes="#//StepTemplate"/>
  <eClassifiers xsi:type="ecore:EClass" name="DataProducer" eSuperTypes="#//StepTemplate"/>
  <eClassifiers xsi:type="ecore:EClass" name="DataConsumer" eSuperTypes="#//StepTemplate"/>
  <eSubpackages name="Trigger">
    <eClassifiers xsi:type="ecore:EClass" name="ScheduleTrigger" eSuperTypes="#//Trigger/Trigger">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="startTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Trigger"/>
    <eClassifiers xsi:type="ecore:EClass" name="ExternalEventTrigger" eSuperTypes="#//Trigger/Trigger"/>
    <eClassifiers xsi:type="ecore:EClass" name="OneTimeTrigger" eSuperTypes="#//Trigger/Trigger">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="startTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CronSchedule" eSuperTypes="#//Trigger/ScheduleTrigger">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="cron" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="IntervalSchedule" eSuperTypes="#//Trigger/ScheduleTrigger">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="frequency" eType="#//TimeUnits"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="interval" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Environment Parameters">
    <eClassifiers xsi:type="ecore:EClass" name="ExecutionRequirement" eSuperTypes="#//Parameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HardParameters" abstract="true" eSuperTypes="#//Environment%20Parameters/ExecutionRequirement"/>
    <eClassifiers xsi:type="ecore:EClass" name="SoftParameters" abstract="true" eSuperTypes="#//Environment%20Parameters/ExecutionRequirement"/>
    <eClassifiers xsi:type="ecore:EClass" name="Metric" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="valueDirection" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isVariable" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
          defaultValueLiteral="false"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="OptimizationRequirement" eSuperTypes="#//Environment%20Parameters/SoftParameters">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="optimisationFunction"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral="MINIMIZE"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="metric" eType="#//Environment%20Parameters/Metric"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HardwareParameter" abstract="true"
        eSuperTypes="#//Environment%20Parameters/HardParameters"/>
    <eClassifiers xsi:type="ecore:EClass" name="ServiceLevelCondition">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="comparisonOperator" eType="#//ComparisonOperatorType"
          defaultValueLiteral="GREATER_THAN"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="threshold" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="validity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HorizontalScaleParameter" eSuperTypes="#//Environment%20Parameters/ScaleParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minInstance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxInstance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SecurityParameter" abstract="true"
        eSuperTypes="#//Environment%20Parameters/HardParameters"/>
    <eClassifiers xsi:type="ecore:EClass" name="OSParameter" eSuperTypes="#//Environment%20Parameters/OSOrImageParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="os" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="is64os" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
          defaultValueLiteral="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ServiceLevelObjective" eSuperTypes="#//Environment%20Parameters/HardParameters">
      <eStructuralFeatures xsi:type="ecore:EReference" name="costumServiceLevel" eType="#//Environment%20Parameters/ServiceLevelCondition"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SecurityControl">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="specification" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="securityParameters" eType="#//Environment%20Parameters/SecurityParameter"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QuantitativeHardwareParameter" eSuperTypes="#//Environment%20Parameters/HardwareParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minCPU" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxCPU" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minRAM" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxRAM" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minStorage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxStorage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ScaleParameter" abstract="true" eSuperTypes="#//Environment%20Parameters/HardParameters"/>
    <eClassifiers xsi:type="ecore:EClass" name="OSOrImageParameter" abstract="true"
        eSuperTypes="#//Environment%20Parameters/HardParameters"/>
    <eClassifiers xsi:type="ecore:EClass" name="ImageParameter" eSuperTypes="#//Environment%20Parameters/OSOrImageParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="imageId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="VerticalScaleParameter" eSuperTypes="#//Environment%20Parameters/ScaleParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minCPU" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxCPU" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minRAM" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxRAM" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minCores" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxCores" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minStorage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxStorage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QualitativeHardwareParameter" eSuperTypes="#//Environment%20Parameters/HardwareParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minBenchmark" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxBenchmark" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Control Parameters">
    <eClassifiers xsi:type="ecore:EClass" name="Condition" eSuperTypes="#//Control%20Parameters/AbstractCondition">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="comparatorOperator" eType="#//ComparisonOperatorType"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="source" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="target" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="sourceProcessingScriptName"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral="None"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="AbstractCondition" instanceClassName="AbstractCondition"
        abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeCondition" eSuperTypes="#//Control%20Parameters/AbstractCondition">
      <eStructuralFeatures xsi:type="ecore:EReference" name="conditions" upperBound="-1"
          eType="#//Control%20Parameters/AbstractCondition" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//LogicalOperatorType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ControlParemeter" eSuperTypes="#//Parameter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="Loop" eType="#//Control%20Parameters/Loop"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//Control%20Parameters/Condition.1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PreCondition">
      <eStructuralFeatures xsi:type="ecore:EReference" name="conditionStepList" lowerBound="1"
          upperBound="-1" eType="#//Control%20Parameters/ConditionStepPair" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//LogicalOperatorType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PostCondition">
      <eStructuralFeatures xsi:type="ecore:EReference" name="conditionStepList" lowerBound="1"
          upperBound="-1" eType="#//Control%20Parameters/ConditionStepPair" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConditionStepPair">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" eType="#//AbstractCondition"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="step" eType="#//Step"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="While" eSuperTypes="#//Control%20Parameters/Loop">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" eType="#//AbstractCondition"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="atLeastOnce" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
          defaultValueLiteral="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Iteration" eSuperTypes="#//Control%20Parameters/Loop">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" eType="#//AbstractCondition"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="processingScript" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral="None"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Loop"/>
    <eClassifiers xsi:type="ecore:EClass" name="Condition">
      <eStructuralFeatures xsi:type="ecore:EReference" name="pre" eType="#//Control%20Parameters/PreCondition"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="post" eType="#//Control%20Parameters/PostCondition"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Deployment Parameters (Requirements)">
    <eClassifiers xsi:type="ecore:EClass" name="DeploymentParameter" eSuperTypes="#//Parameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="layer" eType="#//DeploymentLayers"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Input Parameters">
    <eClassifiers xsi:type="ecore:EClass" name="DataSource" eSuperTypes="#//Parameter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="value" eType="#//Input%20Parameters/ParameterValueTypes"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ParameterValueTypes"/>
  </eSubpackages>
</ecore:EPackage>
